todo = ["eat", "sleep", "repeat"]
in_progress = ["", "", ""]
done = ["", "", ""]

print(todo)
print(in_progress)
print(done, "\n")
in_progress[1] = todo.pop(1)
todo.insert(1, "")
print(todo)
print(in_progress)
print(done, "\n")
done[0] = in_progress.pop(1)
in_progress.insert(1, "")
print(todo)
print(in_progress)
print(done, "\n")

